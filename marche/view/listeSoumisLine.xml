            <field name="name">Marche soumis line</field>
            <field name="model">liste.marche.soumis.line</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="intitule"/>
                    <field name="structure_id"/>
                    <field name="liste_id"/>
                    <field name="dte_depot"/>
                    <field name="type_id"/>

                </tree>
            </field>
    </record>

    <record model="ir.ui.view" id="ListeMarcheSoumisLine_form_view">
            <field name="name">Marche soumis line</field>
            <field name="model">liste.marche.soumis.line</field>
            <field name="arch" type="xml">
                <form>
                    <field name="intitule"/>
                    <field name="structure_id"/>
                    <field name="liste_id"/>
                    <field name="dte_depot"/>
                    <field name="type_id"/>

                </form>
            </field>
    </record>

    <record model="ir.actions.act_window" id="ListeMarcheSoumisLine_action_view">
        <field name="name">Marche soumis line</field>
        <field name="res_model">liste.marche.soumis.line</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>